[difftool]
[push]
	default = current
[alias]
  a = add .
  s = status
  b = branch
  c = commit
  cm = commit -m
  co = checkout
  cob = checkout -b
  l = log
  ll = log -n 1
  sl = shortlog -n
  dt = difftool
[difftool "Kaleidoscope"]
[diff]
